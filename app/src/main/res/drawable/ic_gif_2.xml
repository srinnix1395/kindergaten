<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#4d000000"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#ffffff"
                android:dashGap="5dp"
                android:dashWidth="5dp"/>
        </shape>
    </item>
</layer-list>